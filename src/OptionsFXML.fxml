<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="227.0" prefWidth="319.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="hangmanfx.OptionsController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="21.0" prefWidth="152.0" text="Options:" />
      <RadioButton fx:id="playNormal" layoutX="14.0" layoutY="55.0" mnemonicParsing="false" text="Play with yourself or another partner" />
      <RadioButton fx:id="playAI" layoutX="14.0" layoutY="80.0" mnemonicParsing="false" text="Play with AI" />
      <Button fx:id="playBtn" layoutX="147.0" layoutY="188.0" mnemonicParsing="false" onAction="#playBtnAction" text="Play" />
   </children>
</AnchorPane>
